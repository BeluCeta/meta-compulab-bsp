From c752d2fc608f83ddae6a15718c3f7804898002e0 Mon Sep 17 00:00:00 2001
From: Valentin Raevsky <valentin@compulab.co.il>
Date: Sun, 4 Nov 2018 16:54:46 +0200
Subject: [PATCH 096/103] compulab-imx8mq: Define eeprom_wp regulator

Signed-off-by: Valentin Raevsky <valentin@compulab.co.il>
---
 arch/arm64/boot/dts/compulab/compulab-imx8mq.dtsi | 11 +++++++++++
 1 file changed, 11 insertions(+)

diff --git a/arch/arm64/boot/dts/compulab/compulab-imx8mq.dtsi b/arch/arm64/boot/dts/compulab/compulab-imx8mq.dtsi
index cfe62e8..b1a8d45 100644
--- a/arch/arm64/boot/dts/compulab/compulab-imx8mq.dtsi
+++ b/arch/arm64/boot/dts/compulab/compulab-imx8mq.dtsi
@@ -34,6 +34,16 @@
 		#address-cells = <1>;
 		#size-cells = <0>;
 
+		reg_eeprom_wp: eeprom_wp {
+			compatible = "regulator-fixed";
+			regulator-name = "eeprom_wp";
+			regulator-min-microvolt = <3300000>;
+			regulator-max-microvolt = <3300000>;
+			gpio = <&gpio1 13 GPIO_ACTIVE_HIGH>;
+			regulator-always-on;
+			enable-active-high;
+		};
+
 		reg_usdhc2_vmmc: usdhc2_vmmc {
 			compatible = "regulator-fixed";
 			regulator-name = "VSD_3V3";
@@ -159,6 +169,7 @@
 		pinctrl_hog: hoggrp {
 			fsl,pins = <
 				MX8MQ_IOMUXC_GPIO1_IO12_GPIO1_IO12	0x16
+				MX8MQ_IOMUXC_GPIO1_IO13_GPIO1_IO13	0x16
 			>;
 		};
 
